{
    "name": "mklocke/hexagonal-playground",
    "type": "project",
    "authors": [
        {
            "name": "Marius Klocke",
            "email": "marius.klocke@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HexagonalPlayground\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HexagonalPlayground\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.4",
        "ext-apcu": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "doctrine/dbal": "^4.4.1",
        "doctrine/orm": "~3.6.0",
        "firebase/php-jwt": "^7.0.2",
        "guzzlehttp/guzzle": "^7.10",
        "hollodotme/fast-cgi-client": "^3.1.7",
        "middlewares/trailing-slash": "^2.1.0",
        "nyholm/psr7": "^1.8.2",
        "nyholm/psr7-server": "^1.1.0",
        "php-di/php-di": "^7.1.1",
        "psr/http-client": "^1.0.3",
        "psr/log": "^3.0.2",
        "slim/slim": "^4.15.1",
        "spiral/roadrunner-http": "^3.6",
        "spiral/roadrunner-metrics": "^3.3",
        "symfony/console": "^7.4.1",
        "symfony/mailer": "^8.0.0",
        "webonyx/graphql-php": "^15.27.2"
    },
    "require-dev": {
        "deptrac/deptrac": "^4.4.0",
        "devizzent/cebe-php-openapi": "^1.1.4",
        "league/openapi-psr7-validator": "^0.22.0",
        "phpunit/phpunit": "^12.5.3",
        "robiningelbrecht/phpunit-pretty-print": "^1.4.1"
    }
}
